@article{nagaoHydrogenenhancedplasticityMediatedDecohesion2018,
  title = {Hydrogen-Enhanced-Plasticity Mediated Decohesion for Hydrogen-Induced Intergranular and “Quasi-Cleavage” Fracture of Lath Martensitic Steels},
  author = {Nagao, Akihide and Dadfarnia, Mohsen and Somerday, Brian P. and Sofronis, Petros and Ritchie, Robert O.},
