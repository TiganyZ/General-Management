@article{yuOriginDramaticOxygen2015,
  title = {Origin of Dramatic Oxygen Solute Strengthening Effect in Titanium},
  author = {Yu, Qian and Qi, Liang and Tsuru, Tomohito and Traylor, Rachel and Rugg, David and Morris, J. W. and Asta, Mark and Chrzan, D. C. and Minor, Andrew M.},
  date = {2015-02-06},
