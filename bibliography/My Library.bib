  langid = {english},
  number = {2098}
}

@article{connetableDFTStudySolubility2014,
  title = {{{DFT}} Study of the Solubility of Hydrogen and Carbon in {{Ni3Nb}}-{{D0a}} and {{Ni3Nb}}-{{D022}} Systems},
  author = {Connétable, Damien and Galliano, Florian and Odemer, Grégory and Blanc, Christine and Andrieu, Éric},
