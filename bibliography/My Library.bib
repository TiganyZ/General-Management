@article{shahbaCrystalPlasticityFE2016,
  title = {Crystal Plasticity {{FE}} Modeling of {{Ti}} Alloys for a Range of Strain-Rates. {{Part I}}: {{A}} Unified Constitutive Model and Flow Rule},
